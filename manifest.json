{
  "manifest_version": 3,
  "name": "Google Maps Lead Scraper",
  "version": "3.1.0",
  "description": "Profesjonalne narzÄ™dzie do pobierania danych o firmach z Google Maps. Eksport do CSV, filtry, czysty interfejs.",
  "author": "Your Name",
  
  "permissions": [
    "activeTab",
    "scripting"
  ],
  
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "default_title": "Google Maps Scraper"
  },
  
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  
  "host_permissions": [
    "https://www.google.com/maps/*",
    "https://www.google.pl/maps/*"
  ]
}
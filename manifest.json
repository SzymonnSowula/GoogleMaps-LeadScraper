{
  "manifest_version": 3,
  "name": "Google Maps Lead Scraper Pro",
  "version": "3.0.0",
  "description": "Profesjonalny scraper firm z Google Maps z filtrami, gwiazdkami, godzinami otwarcia i eksportem do CSV.",
  "author": "Your Name",
  
  "permissions": [
    "activeTab",
    "scripting"
  ],
  
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "default_title": "Scraper Firm Google Maps"
  },
  
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  
  "host_permissions": [
    "https://www.google.com/maps/*",
    "https://www.google.pl/maps/*"
  ]
}